# ========================================
# EKS Dummy Project - Variables de Entorno
# ========================================
# Copia este archivo a .env y configura tus valores

# AWS Configuration
AWS_PROFILE=default
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_SESSION_TOKEN=your_session_token_here

# EKS Cluster Configuration
CLUSTER_NAME=eks-dummy-test
CLUSTER_REGION=us-east-1

# Terraform Configuration
TF_VAR_cluster_name=eks-dummy-test
TF_VAR_region=us-east-1
TF_VAR_environment=dummy

# Kubernetes Configuration
KUBECONFIG_PATH=~/.kube/config
NAMESPACE_PREFIX=test

# Monitoring Configuration
CLOUDWATCH_LOG_GROUP_PREFIX=/aws/eks/eks-dummy-test
MONITORING_INTERVAL=30

# Scripts Configuration
DEBUG_MODE=false
DRY_RUN=false
TIMEOUT_SECONDS=300

# MCP Server Configuration
MCP_SERVER_PATH=/home/USERNAME/.local/bin/uvx
MCP_SERVER_PACKAGE=awslabs.eks-mcp-server@latest

# Resource Limits (for testing scenarios)
TEST_CPU_LIMIT=1000m
TEST_MEMORY_LIMIT=1Gi
TEST_CPU_REQUEST=500m
TEST_MEMORY_REQUEST=512Mi 